{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./dist",
    "module": "commonjs",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}
